
public class FlowControlDemo {
	// flow control
	// 1. if statement
	//		1.a simple if
	//		1.b if..else
	// 		1.c else if ladder
	//		1.d nested if
	// 2. switch case statement
	public static void main(String[] args) {
		char ch = 'e';
		String result = null;
		switch(ch) {
		case 'a','e','i','o','u':
			result = "a vowel";
			break;
		default:
			result = "not a vowel";
		}
		System.out.println(ch + " is " + result);
	}

}
