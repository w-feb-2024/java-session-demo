
public class ThrowDemo {
	public static void main(String[] args) {
		
			ArithmeticException ae = new ArithmeticException();
			//throw ae; // used to explicitly throw an exception
		
			Room room = null;
			if(room != null)
				room.computeFloorArea();
			
			
			int a = 5;
			int b = 0;
			if(b != 0) {
				int c = a/b;
			}
		
	}
}
